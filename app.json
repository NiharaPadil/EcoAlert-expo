{
  "expo": {
    "name": "ecoalert-app",
    "slug": "ecoalert",
    "version": "1.0.0",
    "sdkVersion": "51.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/Splash1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Images/Splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/Splash1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.niharapadil.ecoalert"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/Images/Splash1.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "98561167-bfa6-4041-b211-9228b33b6e52"
      }
    },
    "owner": "niharapadil"
  }
}
